{
  "name": "akbsit/nova-field-system-file",
  "description": "Uploading files to the server from nova.",
  "keywords": [
    "laravel",
    "media",
    "files",
    "images",
    "downloads",
    "cms",
    "upload",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0|^10.0|^11.0",
    "laravel/nova": "^3.0|^4.0",
    "akbsit/laravel-system-file": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Akbsit\\NovaFieldSystemFile\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Akbsit\\NovaFieldSystemFile\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
